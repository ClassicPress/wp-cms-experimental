		"@wordpress/hooks": "3.17.1",
		"@wordpress/i18n": "4.17.1",
		"@wordpress/icons": "9.8.1",
		"backbone": "1.4.1",
		"clipboard": "2.0.10",
		"core-js-url-browser": "3.6.4",
		"element-closest": "^2.0.2",
		"formdata-polyfill": "4.0.10",
		"hoverintent": "2.2.1",
		"imagesloaded": "4.1.4",
		"jquery": "3.6.3",
		"jquery-color": "2.2.0",
		"jquery-form": "4.3.0",
